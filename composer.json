{
    "name": "react-parallel/react-parallel",
    "description": "\ud83c\udf09 Bridging ext-parallel and ReactPHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-parallel": "*",
        "react-parallel/contracts": "^1.0",
        "react-parallel/event-loop": "^1.1",
        "react-parallel/infinite-pool": "^2.1",
        "react-parallel/limited-pool": "^1.0",
        "react-parallel/runtime": "^2.0",
        "react-parallel/streams": "^1.0",
        "react/event-loop": "^1.1",
        "react/promise": "^2.8",
        "wyrihaximus/constants": "^1.5",
        "wyrihaximus/metrics": "^1"
    },
    "require-dev": {
        "ancarda/psr7-string-stream": "^1.3",
        "moneyphp/money": "^3.3",
        "ocramius/package-versions": "^1.9 || ^2.0",
        "react/http": "^1.1",
        "wyrihaximus/async-test-utilities": "^3.2",
        "wyrihaximus/iterator-or-array-to-array": "^1.1"
    },
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ReactParallel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReactParallel\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
