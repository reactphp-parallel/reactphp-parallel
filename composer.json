{
    "name": "react-parallel/react-parallel",
    "description": "\ud83c\udf09 Bridging ext-parallel and ReactPHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-parallel": "*",
        "react-parallel/future-to-promise-converter": "dev-master",
        "react-parallel/infinite-pool": "dev-master",
        "react-parallel/limited-pool": "dev-master",
        "react-parallel/runtime": "dev-master",
        "react/event-loop": "^1.1",
        "react/promise": "^2.7",
        "wyrihaximus/constants": "^1.4.3"
    },
    "require-dev": {
        "moneyphp/money": "^3.2 || ^3.2.1",
        "ocramius/package-versions": "^1.4",
        "wyrihaximus/async-test-utilities": "^2.0.0-alpha1",
        "wyrihaximus/iterator-or-array-to-array": "^1.1"
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ReactParallel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReactParallel\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
